{
    "version": 2,
    "builds": [
      { "src": "server/server.js", "use": "@vercel/node" },
      { "src": "ui-server/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server/server.js" },
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "ui-server/dist/index.html" }
    ]
  }
  